<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.xmltest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.provider.Telephony.SMS_RECEIVED"/>
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_elephant"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.LoginStyle">
        <activity android:name=".SelectMomOrSon"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccountActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".YoutubePlayer"
            android:screenOrientation="portrait"/>
        <activity android:name=".VideoYoutube.ViewAllOnCategory"/>
        <activity
            android:name=".NavigationDrawer"
            android:label="@string/title_activity_navigation_drawer"
            android:theme="@style/AppTheme.LoginStyle"
            android:screenOrientation="portrait"/>
        <activity android:name=".SetupAccountActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.EduMainActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.ToanChia"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.ToanCong"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.ToanDem"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.ToanNhan"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Education.ToanTru"
            android:screenOrientation="portrait"></activity>
        <receiver android:name=".SonReceiveAndSendBackService">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_map_key" />

        <uses-feature
            android:glEsVersion="0x00020000"
            android:required="true" />


    </application>

</manifest>